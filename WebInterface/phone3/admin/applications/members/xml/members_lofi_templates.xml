				<else />
					<if test="cf_icq:|:$f->raw_data['pf_key'] == 'icq'">
						<a class='url' type="application/x-icq" href='http://www.icq.com/people/cmd.php?uin={$f->parsed}&amp;action=message'>{$f->parsed}</a>
					<else />
						<if test="cf_website:|:$f->raw_data['pf_key'] == 'website'">
